# ficheiro de configuração para o dominio de topo animalia
# funciona na porta 1222
# é servidor de topo de animalia

# base de dados de animalia
animalia DB dns_local/animalia.db
animalia LG logs_local/animalia.log
# secundários de animalia
animalia SS 127.0.0.1


root ST dns_local/rootServers.db
all LG logs_local/SDT1.log